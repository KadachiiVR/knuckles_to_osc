{
    "action_manifest_version": 0,
    "alias_info": {},
    "app_key" : "kadachii.knuckles_to_osc",

    "bindings": {
        "/actions/main": {
            "skeleton": [
                {
                    "output": "/actions/main/in/LeftHandSkeleton",
                    "path": "/user/hand/left/input/skeleton/left"
                },
                {
                    "output": "/actions/main/in/RightHandSkeleton",
                    "path": "/user/hand/right/input/skeleton/right"
                }
            ],
            "sources": [
                {
                    "inputs": {
                        "position": {
                            "output": "/actions/main/in/TouchPosLeft"
                        },
                        "touch": {
                            "output": "/actions/main/in/TouchTapLeft"
                        },
                        "click": {
                            "output": "/actions/main/in/TouchPressLeft"
                        }
                    },
                    "mode": "trackpad",
                    "path": "/user/hand/left/input/trackpad"
                },
                {
                    "inputs": {
                        "position": {
                            "output": "/actions/main/in/TouchPosRight"
                        },
                        "touch": {
                            "output": "/actions/main/in/TouchTapRight"
                        },
                        "click": {
                            "output": "/actions/main/in/TouchPressRight"
                        }
                    },
                    "mode": "trackpad",
                    "path": "/user/hand/right/input/trackpad"
                },
                {
                    "inputs": {
                        "east": {
                            "output": "/actions/main/in/VictoryLeft"
                        },
                        "north": {
                            "output": "/actions/main/in/ThumbsUpLeft"
                        },
                        "west": {
                            "output": "/actions/main/in/HandGunLeft"
                        },
                        "south": {
                            "output": "/actions/main/in/HandOpenLeft"
                        }
                    },
                    "mode": "dpad",
                    "parameters": {
                        "sub_mode": "touch"
                    },
                    "path": "/user/hand/left/input/trackpad"
                },
                {
                    "inputs": {
                        "east": {
                            "output": "/actions/main/in/RockNRollLeft"
                        },
                        "north": {
                            "output": "/actions/main/in/FistLeft"
                        },
                        "west": {
                            "output": "/actions/main/in/FingerPointLeft"
                        }
                    },
                    "mode": "dpad",
                    "parameters": {
                        "sub_mode": "click"
                    },
                    "path": "/user/hand/left/input/trackpad"
                },
                {
                    "inputs": {
                        "east": {
                            "output": "/actions/main/in/VictoryRight"
                        },
                        "north": {
                            "output": "/actions/main/in/ThumbsUpRight"
                        },
                        "west": {
                            "output": "/actions/main/in/HandGunRight"
                        },
                        "south": {
                            "output": "/actions/main/in/HandOpenRight"
                        }
                    },
                    "mode": "dpad",
                    "parameters": {
                        "sub_mode": "touch"
                    },
                    "path": "/user/hand/right/input/trackpad"
                },
                {
                    "inputs": {
                        "east": {
                            "output": "/actions/main/in/RockNRollRight"
                        },
                        "north": {
                            "output": "/actions/main/in/FistRight"
                        },
                        "west": {
                            "output": "/actions/main/in/FingerPointRight"
                        }
                    },
                    "mode": "dpad",
                    "parameters": {
                        "sub_mode": "click"
                    },
                    "path": "/user/hand/right/input/trackpad"
                }
            ]
        }
    },
    "category" : "steamvr_input",
    "controller_type" : "knuckles",
    "description" : "Change the DPad stuff if you want to customize your gesture emulation",
    "name" : "Default Index Controller Binding",
    "options" : {},
    "simulated_actions" : []
}